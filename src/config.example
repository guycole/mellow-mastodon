#
# repository of processed observations
archiveDir: /var/mellow/mastodon/archive
#
# repository of converted observations
cookedDir: /var/mellow/mastodon/cooked
#
# repository of problem files
failureDir: /var/mellow/mastodon/failure
#
# repository of freshly collected observations
freshDir: /var/mellow/mastodon/fresh
#
# processed artifacts
processedDir: /var/mellow/mastodon/processed
#
# test artifacts
testDir: /var/mellow/mastodon/test
#
# database connection
dbConn: "postgresql+psycopg2://mastodon_py:bogus@localhost:5432/mastodon_v1"
#
# true, echo SQL statements
sqlEchoEnable: true
#
# true JSON output is only peakers else all bins
peakerOnly: true
#
# true, enable test mode
testModeEnable: true
